{
  "name": "bitbucket-gpg-viewer-chrome-extension",
  "version": "1.0.0",
  "description": "A chrome extension for decoding and viewing pgp encoded files (.gpg) in bitbucket/stash",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/main.ts",
    "build": " parcel build --no-source-maps src/main.ts",
    "package": "zip -r bitbucket-gpg-viewer-chrome-extension dist manifest.json bitbucket-gpg-viewer-chrome-extension.png",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "keywords": [
    "chrome-extension",
    "bitbucket",
    "stash",
    "gpg decoder",
    "gpg viwer",
    "pgp",
    "gpg"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chrome": "0.0.127",
    "@types/codemirror": "0.0.102",
    "@types/openpgp": "4.4.14",
    "@types/react": "17.0.0",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-nuke-dist": "1.0.1",
    "prettier": "2.2.1",
    "react": ">=16.9.0",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "openpgp": "4.10.9",
    "prop-types": "15.7.2",
    "react-diff-view": "2.4.6",
    "react-dom": ">=16.9.0",
    "unidiff": "1.0.2"
  }
}
